{
  "name": "@origin-digital/ods-pickers",
  "description": "Accessible DatePickers for ODS",
  "license": "UNLICENSED",
  "author": "Alex Mordue <alex.mordue@origin.com.au>",
  "version": "1.0.4",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn build:cjs && yarn build:es",
    "build:cjs": "BUILD_FORMAT=cjs origin-scripts build --monorepo --delete-dir-on-start --out-dir dist/cjs",
    "build:es": "BUILD_WEBPACK=true origin-scripts build --monorepo --out-dir dist/es"
  },
  "sideEffects": false,
  "typings": "dist/typings/index.d.ts",
  "dependencies": {
    "@babel/runtime-corejs3": "^7.8.3",
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.11.0",
    "@material-ui/pickers": "^3.2.10",
    "@origin-digital/ods-core": "^1.1.0",
    "@origin-digital/ods-devtools": "^1.0.0",
    "@origin-digital/ods-helpers": "^1.0.3",
    "date-fns": "^2.15.0"
  },
  "peerDependencies": {
    "react": "^16.9.0",
    "styled-components": "3.4.5"
  },
  "devDependencies": {
    "@origin-digital/ods-testing-library": "^1.0.0"
  },
  "publishConfig": {
    "registry": "https://artifactory.origindigital-dac.com.au/artifactory/api/npm/npm-internal/"
  }
}
