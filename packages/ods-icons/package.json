{
  "private": false,
  "name": "@origin-digital/ods-icons",
  "description": "Contains Icons that understand typography and colour context.",
  "license": "UNLICENSED",
  "author": "Nathan Vale <nathan.vale@origin.com.au>",
  "version": "1.0.2",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn build:cjs && yarn build:es",
    "build:cjs": "BUILD_FORMAT=cjs origin-scripts build --monorepo --delete-dir-on-start --out-dir dist/cjs",
    "build:es": "BUILD_WEBPACK=true origin-scripts build --monorepo --out-dir dist/es",
    "create:mui-icons": "cd ../ods-scripts && yarn create:mui-icons"
  },
  "sideEffects": false,
  "typings": "dist/typings/index.d.ts",
  "dependencies": {
    "@babel/runtime-corejs3": "^7.8.3",
    "@material-ui/icons": "^4.9.1",
    "@origin-digital/ods-themes": "^1.0.2",
    "@origin-digital/ods-types": "^1.0.2"
  },
  "peerDependencies": {
    "react": "^16.9.0",
    "styled-components": "3.4.5"
  },
  "publishConfig": {
    "registry": "https://artifactory.origindigital-dac.com.au/artifactory/api/npm/npm-internal/"
  }
}
