    "@origin-digital/ods-icons": "^1.0.3",
    "@origin-digital/ods-themes": "^1.1.0",
    "@origin-digital/ods-types": "^1.1.0",
    "@origin-digital/ods-typography": "^1.0.3",
    "clsx": "^1.1.0",
    "csstype": "^2.6.4",
    "polished": "2.0.3",
    "react-keyed-flatten-children": "^1.2.0",
    "styled-system": "4.2.3",
